{
  "name": "react-setup",
  "version": "1.1.1",
  "private": true,
  "scripts": {
    "start": "npm run build && node ./www-start",
    "debug": "npm run build-server-dev && node ./dist/www-start",
    "build.temp": "npm run build-server-dev && npm run build-client",
    "build": "npm run build-server && npm run build-client",
    "build-server": "webpack --progress --color  --display-error-details  --config webpack-config.js --mode production",
    "build-client": "webpack --progress --color  --display-error-details  --config webpack-config-client.js --mode production",
    "build-server-dev": "webpack --progress --color  --display-error-details  --config webpack-config.js --mode development"
  },
  "dependencies": {
    "cookie-parser": "~1.4.5",
    "debug": "^4.1.1",
    "ejs": "~3.1.5",
    "express": "~4.17.1",
    "firebase": "^7.18.0",
    "http-errors": "^1.8.0",
    "moment-timezone": "^0.5.31",
    "morgan": "^1.10.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-dom-server": "^0.0.5",
    "react-hot-loader": "^4.12.21",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "rxjs": "^6.6.2",
    "tailwindcss": "^1.7.1"
  },
  "devDependencies": {
    "@babel/core": "^7.11.1",
    "@babel/preset-env": "^7.11.0",
    "@babel/preset-react": "^7.10.4",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.7",
    "@types/node": "^14.6.0",
    "@types/react": "^16.9.46",
    "@types/react-dom": "^16.9.8",
    "@types/react-router": "^5.1.8",
    "@types/react-router-dom": "^5.1.5",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^4.2.1",
    "mini-css-extract-plugin": "^0.10.0",
    "node-sass": "^4.14.1",
    "sass-loader": "^9.0.3",
    "style-loader": "^1.2.1",
    "ts-loader": "^8.0.2",
    "typescript": "^3.9.7",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0"
  }
}
