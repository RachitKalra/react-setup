{
  "name": "react-setup",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "start": "npm run build && node ./www-start",
    "debug": "npm run build-server-dev && node ./www-start",
    "build": "npm run build-server && npm run build-client",
    "build-server": "webpack --progress --color  --display-error-details  --config webpack-config.js --mode production",
    "build-client": "webpack --progress --color  --display-error-details  --config webpack-config-client.js --mode production",
    "build-server-dev": "webpack --progress --color  --display-error-details  --config webpack-config.js --mode development"
  },
  "dependencies": {
    "cookie-parser": "^1.4.4",
    "debug": "^4.1.1",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "http-errors": "^1.7.2",
    "moment-timezone": "^0.5.25",
    "morgan": "^1.9.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-dom-server": "0.0.5",
    "react-hot-loader": "^4.8.4",
    "react-router": "^5.0.0",
    "react-router-dom": "^5.0.0",
    "rxjs": "^6.4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-react": "^7.0.0",
    "@types/express": "^4.16.1",
    "@types/node": "^12.0.0",
    "@types/react": "^16.8.16",
    "@types/react-dom": "^16.8.4",
    "@types/react-router": "^4.4.5",
    "@types/react-router-dom": "^4.3.3",
    "babel-loader": "^8.0.5",
    "css-loader": "^2.1.1",
    "mini-css-extract-plugin": "^0.6.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "ts-loader": "^6.0.0",
    "typescript": "^3.4.5",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-middleware": "^3.6.2",
    "webpack-hot-middleware": "^2.24.4"
  }
}
