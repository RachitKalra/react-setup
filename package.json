{
  "name": "react-setup",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "start": "npm run build && node ./www-start",
    "debug": "npm run build-server-dev && node ./www-start",
    "build": "npm run build-server && npm run build-client",
    "build-server": "webpack --progress --color  --display-error-details  --config webpack-config.js --mode production",
    "build-client": "webpack --progress --color  --display-error-details  --config webpack-config-client.js --mode production",
    "build-server-dev": "webpack --progress --color  --display-error-details  --config webpack-config.js --mode development"
  },
  "dependencies": {
    "cookie-parser": "^1.4.4",
    "debug": "^4.1.1",
    "ejs": "^2.6.2",
    "express": "~4.17.1",
    "http-errors": "^1.7.3",
    "moment-timezone": "^0.5.25",
    "morgan": "^1.9.1",
    "react": "~16.9.0",
    "react-dom": "~16.9.0",
    "react-dom-server": "0.0.5",
    "react-hot-loader": "~4.12.10",
    "react-router": "~5.0.1",
    "react-router-dom": "~5.0.1",
    "rxjs": "~6.5.2"
  },
  "devDependencies": {
    "@babel/core": "~7.5.5",
    "@babel/preset-env": "~7.5.5",
    "@babel/preset-react": "~7.0.0",
    "@types/express": "~4.17.0",
    "@types/node": "~12.7.1",
    "@types/react": "^16.9.1",
    "@types/react-dom": "~16.8.5",
    "@types/react-router": "~5.0.3",
    "@types/react-router-dom": "~4.3.4",
    "babel-loader": "~8.0.6",
    "css-loader": "~3.2.0",
    "mini-css-extract-plugin": "~0.8.0",
    "node-sass": "^4.12.0",
    "sass-loader": "~7.2.0",
    "style-loader": "~1.0.0",
    "ts-loader": "~6.0.4",
    "typescript": "~3.5.3",
    "webpack": "~4.39.1",
    "webpack-cli": "~3.3.6",
    "webpack-dev-middleware": "~3.7.0",
    "webpack-hot-middleware": "~2.25.0"
  }
}
