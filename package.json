{
  "name": "react-setup",
  "version": "1.1.0",
  "private": true,
  "scripts": {
    "start": "npm run build && node ./www-start",
    "debug": "npm run build-server-dev && node ./www-start",
    "build": "npm run build-server && npm run build-client",
    "build-server": "webpack --progress --color  --display-error-details  --config webpack-config.js --mode production",
    "build-client": "webpack --progress --color  --display-error-details  --config webpack-config-client.js --mode production",
    "build-server-dev": "webpack --progress --color  --display-error-details  --config webpack-config.js --mode development"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "^4.1.1",
    "ejs": "~3.0.1",
    "express": "~4.17.1",
    "http-errors": "^1.7.3",
    "moment-timezone": "^0.5.27",
    "morgan": "^1.9.1",
    "react": "~16.12.0",
    "react-dom": "~16.12.0",
    "react-dom-server": "0.0.5",
    "react-hot-loader": "~4.12.18",
    "react-router": "~5.1.2",
    "react-router-dom": "~5.1.2",
    "rxjs": "~6.5.3"
  },
  "devDependencies": {
    "@babel/core": "~7.7.7",
    "@babel/preset-env": "~7.7.7",
    "@babel/preset-react": "~7.7.4",
    "@types/express": "~4.17.2",
    "@types/node": "~12.12.21",
    "@types/react": "^16.9.17",
    "@types/react-dom": "~16.9.4",
    "@types/react-router": "~5.1.3",
    "@types/react-router-dom": "~5.1.3",
    "babel-loader": "~8.0.6",
    "css-loader": "~3.4.0",
    "mini-css-extract-plugin": "~0.9.0",
    "node-sass": "^4.13.0",
    "sass-loader": "~8.0.0",
    "style-loader": "~1.1.1",
    "ts-loader": "~6.2.1",
    "typescript": "~3.5.3",
    "webpack": "~4.41.4",
    "webpack-cli": "~3.3.10",
    "webpack-dev-middleware": "~3.7.2",
    "webpack-hot-middleware": "~2.25.0"
  }
}
